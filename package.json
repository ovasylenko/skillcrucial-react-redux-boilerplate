{
  "name": "boilerplate-skillcrucial",
  "version": "0.0.1",
  "description": "Boilerplate SkillCrucial",
  "main": "index.js",
  "scripts": {
    "docker-start-dev": "docker-compose -f \"docker\\DEV.docker-compose.yml\" up",
    "docker-stop-dev": "docker-compose -f \"docker\\DEV.docker-compose.yml\" down",
    "dev": "webpack-dev-server --mode development --config ./webpack.development.config.js --hot --open --disable-host-check",
    "dev:server": "webpack  --watch --config webpack.development.config.js",
    "start:dev": "webpack-dev-server --mode development --config ./webpack.development.config.js --hot --open",
    "start": "cross-env NODE_ENV=production && node start.js",
    "clean": "rimraf dist",
    "build": "cross-env npm run clean && webpack --config webpack.production.config.js --progress --profile --colors",
    "lint": "eslint ./client/**/**.js ./server/**/**.js ",
    "prettier": "prettier ./client/**/**.js ./server/**/**.js --write ",
    "lint:server": "eslint ./server/**/**.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "next:build": "next",
    "watch:server": "nodemon --legacy-watch start.js --ignore \"*.hot-update.json\" --ignore \"js/bundle.js\" --ignore \"css/main.css\" --watch server --watch client --exec \"npm run lint:server  --scripts-prepend-node-path && node\"",
    "build:prod": "cross-env NODE_ENV=production webpack --config webpack.production.config.js --progress --profile --colors",
    "heroku-postbuild": "yarn run build:prod",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "",
  "license": "MIT",
  "jest": {
    "setupFiles": [
      "<rootDir>/config/jest/shim.js"
    ],
    "collectCoverageFrom": [
      "app/**/*.{js,jsx,ts,tsx}"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/config/jest/assetsTransformer.js"
    },
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ],
    "transform": {
      "\\.js$": "babel-jest"
    }
  },
  "dependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-export-default-from": "^7.7.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.7.4",
    "@babel/plugin-proposal-function-sent": "^7.7.4",
    "@babel/plugin-proposal-json-strings": "^7.7.4",
    "@babel/plugin-proposal-numeric-separator": "^7.7.4",
    "@babel/plugin-proposal-throw-expressions": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-syntax-import-meta": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-flow": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-stage-3": "^7.0.0",
    "@babel/preset-typescript": "^7.7.4",
    "@babel/register": "^7.7.4",
    "@babel/runtime": "^7.7.6",
    "@storybook/cli": "^5.2.8",
    "autoprefixer": "^9.7.3",
    "axios": "^0.19.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-plugin-emotion": "^10.0.23",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react-boilerplate": "1.1.1",
    "babel-preset-react-hmre": "1.1.1",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.6.1",
    "cookie-parser": "^1.4.4",
    "core-js": "^3.5.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.0",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "git-revision-webpack-plugin": "^3.0.4",
    "history": "^4.10.1",
    "next": "^9.3.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "purgecss-webpack-plugin": "^1.6.0",
    "raf": "^3.4.1",
    "raw-loader": "^4.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.18",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "sockjs": "^0.3.19",
    "sockjs-client": "^1.4.0",
    "string-replace-webpack-plugin": "^0.1.3",
    "tailwindcss": "^1.2.0",
    "terser-webpack-plugin": "^2.3.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "3.3.2",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "file-loader": "5.0.2",
    "glob": "^7.1.6",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "3.2.0",
    "husky": "3.1.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-hot-loader": "^1.17.7",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "purgecss-webpack-plugin": "^1.6.0",
    "react-addons-test-utils": "^15.6.2",
    "regenerator-runtime": "^0.13.3",
    "rimraf": "3.0.0",
    "sass-loader": "8.0.0",
    "style-loader": "1.0.1",
    "terser-webpack-plugin": "^2.3.0",
    "thread-loader": "^2.1.3",
    "url-loader": "3.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-shell-plugin": "^0.5.0"
  },
  "nodemonConfig": {
    "ignore": [
      "*.hot-update.json"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "cross-env npm run lint && npm test",
      "pre-push": "cross-env npm run lint && npm test"
    }
  }
}
